  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg": true,
    "**/.history": true,
    "**/.httr-oauth": true,
    "**/.RData": true,
    "**/.Rhistory": true,
    "**/.Rproj.user": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/dist/**": true,
    "**/docs/**": false,
    "**/log/**": true,
    "**/logs/**": true,
    "**/man/**": true,
    "**/pdfs/**": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.Rproj.user": true,
    "**/dist/**": true,
    "**/docs/**": true,
    "**/log/**": true,
    "**/logs/**": true,
    "**/man/**": true,
    "**/pdfs/**": true,
    "**/renv/**": true,
    "**/tmp/**": true
  },
  "git.enableCommitSigning": true,
  "git.inputValidation": "always",
  "github-actions.workflows.pinned.refresh.enabled": true,
  "github-actions.workflows.pinned.refresh.interval": 500,
  "github-actions.workflows.pinned.workflows": [
    ".github/workflows/test-coverage.yaml",
    ".github/workflows/ci.yaml",
    ".github/workflows/binder.yaml"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "codemeta.json"
      ],
      "url": "https://raw.githubusercontent.com/codemeta/codemeta/master/codemeta.jsonld"
    }
  ],
  "latex-workshop.bibtex-format.sort.enabled": true,
  "latex-workshop.bibtex-format.sortby": [
    "key",
    "title"
  ],
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.view.pdf.viewer": "tab",
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "markdownlint.config": {
    "default": true,
    "MD003": {
      "style": "atx"
    },
    "MD007": {
      "indent": 2
    },
    "MD013": false,
    "MD025": false,
    "MD030": {
      "ol_multi": 2,
      "ol_single": 2,
      "ul_multi": 2,
      "ul_single": 2
    },
    "MD031": false,
    "MD033": {
      "allowed_elements": [
        "center",
        "img"
      ]
    },
    "MD045": false
  },
  "markdownlint.lintWorkspaceGlobs": [
    "**/*.{md,mkd,mdwn,mdown,markdown,markdn,mdtxt,mdtext,workbook}",
    "!**/node_modules",
    "!**/bower_components",
    "!**/.git"
  ],
  "markdownlint.run": "onSave",
  "r.alwaysUseActiveTerminal": true,
  "r.lsp.diagnostics": true,
  "r.rmarkdown.knit.focusOutputChannel": false,
  "r.session.levelOfObjectDetail": "Detailed",
  "reflowMarkdown.preferredLineLength": 120,
  "search.exclude": {
    "**/_*/": true
  },
  "sortJSON.orderOverride": [
    "label",
    "type"
  ],
  "sortJSON.orderUnderride": [
    "problemMatcher"
  ],
  "taskExplorer.enableAnsiconForAnt": false,
  "taskExplorer.enableSideBar": true,
  "taskExplorer.exclude": [
    "**/dist/**",
    "**/log/**",
    "**/logs/**",
    "**/pdfs/**",
    "**/renv/**",
    "**/man/**",
    "**/docs/**",
    "**/tmp/**",
    "**/.github/**",
    "**/dev/**",
    "/workspace/false.alarm/run.sh",
    "/workspace/false.alarm/.devcontainer/configure_gitpod.sh",
    "**/.trunk/**"
  ],
  "taskExplorer.groupMaxLevel": 2,
  "taskExplorer.groupSeparator": ": ",
  "taskExplorer.showLastTasks": false,
  "todo-tree.filtering.includeGlobs": [
    "**/src/**",
    "**/R/**",
    "**/scripts/**",
    "**/index/**",
    "**/analysis/**",
    "**/data/**",
    "**/protocol/**",
    "**/review/**",
    "**/tests/**",
    "./**"
  ],
  "todo-tree.filtering.useBuiltInExcludes": "file and search excludes",
  "todo-tree.general.enableFileWatcher": true,
  "todo-tree.general.statusBar": "tags",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "IMPROVE",
    "CONCEPT",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.useColourScheme": true,
  "todo-tree.ripgrep.ripgrep": "/usr/bin/rg",
  "todo-tree.tree.buttons.export": true,
  "todo-tree.tree.disableCompactFolders": true,
  "todo-tree.tree.expanded": true,
  "todo-tree.tree.scanMode": "current file",
  "todo-tree.tree.showBadges": true,
  "todo-tree.tree.showCountsInTree": true,
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": "file:///workspace/false.alarm/.github/workflows/ci.yaml"
  },
  "PandocCiter.DefaultBib": "papers/references.bib",
  "grammarly.config.documentDialect": "american",
  "grammarly.config.documentDomain": "academic",
  "grammarly.config.suggestions.ConjunctionAtStartOfSentence": true,
  "grammarly.config.suggestions.InformalPronounsAcademic": false,
  "grammarly.config.suggestions.OxfordComma": false,
  "grammarly.config.suggestions.PassiveVoice": false,
  "grammarly.startTextCheckInPausedState": true,
  "grammarly.config.suggestions.PunctuationWithQuotation": false,
  "svg.preview.background": "editor",
  "taskExplorer.enabledTasks": {
    "ant": false,
    "bash": false,
    "batch": false,
    "composer": false,
    "gradle": false,
    "grunt": false,
    "gulp": false,
    "make": false,
    "maven": false,
    "npm": false,
    "perl": false,
    "pipenv": false,
    "powershell": false,
    "python": false,
    "ruby": false,
    "tsc": false
  }
